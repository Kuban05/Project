@model IEnumerable<Library.Models.Category>

<h1>Kategorie:</h1>

@foreach (var item in Model)
{
    <div style="float:left; padding: 20px;">
        <h2>@item.Name</h2>
        <ul>
            @foreach (var book in item.Books)
                {
                <li>
                    Tytuł: @book.Title <br />
                    Autor: @book.Author <br />
                </li>
            }
        </ul>
    </div>
}
<div style="clear:both;"></div>